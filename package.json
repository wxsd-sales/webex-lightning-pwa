{
  "private": true,
  "name": "webex-lightning-pwa",
  "version": "0.0.1",
  "description": "A lightweight Progressive Web App for Webex",
  "keywords": [
    "webex",
    "meetings",
    "calling",
    "teams"
  ],
  "homepage": "https://github.com/askmrsinh/webex-lightning-pwa#readme",
  "bugs": {
    "url": "https://github.com/askmrsinh/webex-lightning-pwa/issues"
